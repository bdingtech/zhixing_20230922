@import "/wxss/common.wxss";
.dialog-sub-title {
  color: #4a4a4a;
  font-weight: 700;
}
.dialog-desc,
.dialog-sub-title {
  display: block;
  font-size: 24rpx;
  text-align: center;
}
.dialog-desc {
  color: #ff9e74;
  margin-top: 8rpx;
}
.dialog-game-pic {
  display: block;
  height: 136rpx;
  margin: 24rpx auto 36rpx;
  width: 136rpx;
}
.dialog-login-btn {
  align-items: center;
  background-image: linear-gradient(45deg, #f99b73, #9473ff);
  border-radius: 4rpx;
  color: #fff;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  height: 68rpx;
  justify-content: center;
  margin: 0 auto 40rpx;
  width: 264rpx;
}
