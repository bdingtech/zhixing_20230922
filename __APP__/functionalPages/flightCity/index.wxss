@import "/functionalPages/sub-vendors.wxss";
.zt-tab {
  background: rgba(0, 0, 0, 0.03);
  border-radius: 16rpx;
  padding: 4rpx;
}
.zt-tab .zt-tab-item {
  color: #666;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  height: 60rpx;
  width: 236rpx;
}
.zt-tab .zt-tab-item.cur {
  background: #fefffe;
  border-radius: 14rpx;
  color: #222;
  font-family: PingFangSC-Medium;
  font-weight: 500;
}
.flight-city-module {
  overflow: hidden;
  position: relative;
}
.flight-city-module .city-module-wrapper {
  height: 100%;
  position: relative;
}
.flight-city-module .city-module_list {
  height: 100%;
}
.flight-city-module .city-tag {
  background: #fff;
  color: #999;
  font-size: 24rpx;
  line-height: 50rpx;
  padding: 12rpx 0 20rpx;
  z-index: 1;
}
.flight-city-module .city-tag.top {
  left: 0;
  padding-bottom: 0;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 1000;
}
.flight-city-module .city-module_items {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
}
.flight-city-module .city-module_items::after {
  clear: both;
  content: ".";
  display: block;
  height: 0;
  overflow: hidden;
}
.flight-city-module .item-s {
  -ms-flex-align: center;
  -ms-flex-pack: center;
  align-items: center;
  background: #f7f7f7;
  border-radius: 16rpx;
  color: #222;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  font-family: PingFangSC-Regular;
  height: 68rpx;
  justify-content: center;
  margin: 0 16rpx 16rpx 0;
  position: relative;
  width: 150rpx;
}
.flight-city-module .item-s:nth-of-type(4n) {
  margin-right: 0;
}
.flight-city-module .item-s.cur {
  font-family: PingFangSC-Medium;
}
.flight-city-module .item-s .name {
  font-size: 26rpx;
  line-height: 30rpx;
  text-align: center;
}
.flight-city-module .item-s .tgrey {
  color: #999;
  font-size: 20rpx;
  line-height: 24rpx;
}
.flight-city-module .item-s .tag {
  border-radius: 10rpx 2rpx;
  color: #fff;
  font-size: 18rpx;
  height: 30rpx;
  left: -6rpx;
  line-height: 30rpx;
  padding: 0 8rpx;
  position: absolute;
  top: -16rpx;
}
.flight-city-module .item-s .ifont-location {
  font-size: 25rpx;
  margin-right: 4rpx;
}
.flight-city-module .ifont-arr {
  font-size: 20rpx;
  font-weight: 600;
  margin-left: 4rpx;
  transform: rotate(90deg);
  transform-origin: center;
  transition: -webkit-transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
}
.flight-city-module .ifont-arr.open {
  transform: rotate(-90deg);
}
.flight-city-module .city-module_items.zx .item-s.cur {
  background: rgba(25, 140, 255, 0.05);
  color: #198cff;
}
.flight-city-module .city-module_items.ty .item-s.cur {
  background: rbga(252, 110, 81, 0.05);
  color: #fc6e51;
}
.flt-city-btom {
  background: #fff;
  border-radius: 40rpx 40rpx 0 0;
  box-shadow: 0-2rpx 8rpx 0 rgba(0, 0, 0, 0.04);
  padding: 0 16rpx calc(16rpx + env(safe-area-inset-bottom));
  z-index: 2;
}
.flt-city-btom .city-select {
  height: 108rpx;
  line-height: 108rpx;
  margin: 0 16rpx;
}
.flt-city-btom .label {
  color: #666;
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  font-weight: 400;
  height: 40rpx;
  line-height: 40rpx;
  margin-right: 32rpx;
}
.flt-city-btom .notice {
  color: #ccc;
  font-size: 28rpx;
}
.flt-city-btom .item-box {
  overflow-x: auto;
  scroll-behavior: smooth;
  white-space: nowrap;
}
.flt-city-btom .item {
  background: rgba(25, 140, 255, 0.05);
  border-radius: 16rpx;
  color: #198cff;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  font-weight: 500;
  height: 52rpx;
  margin-right: 12rpx;
  padding: 0 16rpx;
}
.ty .flt-city-btom .item {
  background: rbga(252, 110, 81, 0.05);
  color: #fc6e51;
}
.flt-city-btom .ifont-clear {
  color: #0080ff;
  font-size: 16rpx;
  margin-left: 4rpx;
}
.flt-city-btom .btn-submit {
  background: #0080ff;
  border-radius: 24rpx;
  color: #fff;
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
  font-weight: 600;
  height: 88rpx;
}
.flt-poi-box {
  background: #fff;
  border-radius: 32rpx;
  margin: 0 16rpx 16rpx;
  min-height: 2rpx;
  padding: 32rpx;
}
.flt-poi-box .no-result {
  color: #999;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  font-weight: 400;
  height: 40rpx;
  padding: 18rpx;
  text-align: center;
}
.flt-poi-box .poi-list {
  height: 100%;
}
.flt-poi-box .poi-item {
  box-sizing: border-box;
  height: 92rpx;
  position: relative;
}
.flt-poi-box .poi-item::before {
  content: "";
  display: block;
  height: 2rpx;
  transform: scaleY(0.5);
}
.flt-poi-box .poi-item:first-child::before {
  display: none;
}
.flt-poi-box .poi-tag {
  background: #fff2f2;
  border-radius: 12rpx;
  color: #ff5959;
  font-family: PingFangSC-Medium;
  font-size: 20rpx;
  font-weight: 500;
  height: 36rpx;
  line-height: 36rpx;
  margin-right: 16rpx;
  padding: 0 12rpx;
}
.flt-poi-box .poi-tag.sub {
  background: #edf6ff;
  color: #198cff;
}
.flt-poi-box .poi-txt {
  color: #222;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  font-weight: 500;
  height: 40rpx;
  line-height: 40rpx;
  margin-right: 12rpx;
}
.flt-poi-box .poi-txt.disabled {
  color: #999;
}
.flt-poi-box .sub-txt {
  color: #999;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 32rpx;
}
.flt-poi-box .poi-distance {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translate3d(0, -50%, 0);
}
page {
  background: #f5f5f5;
  overflow-y: hidden;
}
.flight-city ::-webkit-scrollbar {
  background: transparent;
  color: transparent;
  display: none;
  height: 0;
  width: 0;
}
.search-module {
  display: block;
  position: relative;
  z-index: 99;
}
.search-module .ifont2-close {
  color: #ccc;
  font-size: 34rpx;
  margin: -16rpx 12rpx -16rpx -16rpx;
  padding: 16rpx;
  position: relative;
  z-index: 100;
}
.search-input-box {
  background: #fff;
  border-radius: 24rpx;
  height: 88rpx;
  margin: 12rpx 16rpx 24rpx;
}
.search-input-box::after {
  border: 6rpx solid #e6e6e6;
  border-radius: 48rpx;
}
.search-input-box.focus::after {
  border-color: #198cff;
}
.search-module_input {
  background: transparent;
  color: #222;
  font-family: PingFangSC-Semibold;
  font-size: 30rpx;
  font-weight: 600;
  margin-left: 34rpx;
  text-align: left;
}
.search-module_input_focus {
  color: #ccc;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}
.city-content-box {
  background: #fff;
  border-radius: 24rpx;
  margin: 0 16rpx 16rpx;
  min-height: 2rpx;
  padding: 28rpx 32rpx;
}
.city-letter {
  bottom: 0;
  box-sizing: border-box;
  padding-top: 20rpx;
  position: fixed;
  right: 4rpx;
  text-align: center;
  top: 252rpx;
  width: 44rpx;
  z-index: 10;
}
.city-letter-item {
  color: #666;
  display: block;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  font-weight: 400;
  height: 24rpx;
  line-height: 24rpx;
  padding-top: 10rpx;
}
.city-letter-item.txt {
  font-size: 18rpx;
  height: 22rpx;
  line-height: 22rpx;
  padding-top: 12rpx;
}
.city-letter-item:first-child {
  padding-top: 0;
}
.city-letter-item.cur {
  color: #198cff;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
}
.flight-city-tab {
  margin-bottom: 18rpx;
}
.flight-city-tab .intl-tab {
  position: relative;
}
.flight-city-tab .intl-tab .tab-tag {
  animation: shake 2.5s cubic-bezier(0.455, 0.03, 0.515, 0.955) infinite both;
  animation-delay: 800;
  background: #ff5959;
  border-radius: 14rpx 14rpx 14rpx 6rpx;
  color: #fff;
  font-family: PingFangSC-Medium;
  font-size: 18rpx;
  font-weight: 500;
  height: 28rpx;
  padding: 0 10rpx;
  position: absolute;
  right: 16rpx;
  top: -12rpx;
  transform: translateX(100%);
  transform-origin: 100% 100%;
}
@-webkit-keyframes shake {
  100%,
  20% {
    transform: rotate(0deg) translateX(100%);
  }
  28% {
    transform: rotate(2deg) translateX(100%);
  }
  36%,
  52%,
  68% {
    transform: rotate(-4deg) translateX(100%);
  }
  44%,
  60%,
  76% {
    transform: rotate(4deg) translateX(100%);
  }
  84% {
    transform: rotate(-2deg) translateX(100%);
  }
  92% {
    transform: rotate(2deg) translateX(100%);
  }
}
@keyframes shake {
  100%,
  20% {
    transform: rotate(0deg) translateX(100%);
  }
  28% {
    transform: rotate(2deg) translateX(100%);
  }
  36%,
  52%,
  68% {
    transform: rotate(-4deg) translateX(100%);
  }
  44%,
  60%,
  76% {
    transform: rotate(4deg) translateX(100%);
  }
  84% {
    transform: rotate(-2deg) translateX(100%);
  }
  92% {
    transform: rotate(2deg) translateX(100%);
  }
}
