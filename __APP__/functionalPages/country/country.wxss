@import "/functionalPages/sub-vendors.wxss";
.country-search-site {
  background: #f5f5f5;
  bottom: 0;
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
}
.country-search-site .white-box {
  background: #fff;
  padding: 20rpx 30rpx 30rpx;
}
.country-search-site .search-input-bd {
  -ms-flex-align: center;
  -ms-flex-pack: start;
  align-items: center;
  background-color: #f8f8f8;
  border-radius: 34rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 68rpx;
  justify-content: flex-start;
}
.country-search-site .search-input-bd .iconfont {
  color: #999;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  width: 80rpx;
}
.country-search-site .icon-del {
  margin-left: auto;
}
.country-search-site .search-module_input {
  background: transparent;
  box-sizing: border-box;
  flex: 1;
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
}
.country-search-site .search-module_input_focus {
  color: #999;
  font-size: 30rpx;
  line-height: 80rpx;
}
.country-search-site .row {
  -ms-flex-align: center;
  align-items: center;
  border-bottom: 1rpx solid #ededed;
  color: #999;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  font-size: 24rpx;
  height: 100rpx;
}
