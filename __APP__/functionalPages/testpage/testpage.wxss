@import "/functionalPages/sub-vendors.wxss";
.testpage {
  background: #fff;
  min-height: 100vh;
  padding: 30rpx;
}
.testpage .header {
  -ms-flex-pack: center;
  background-color: #0080ff;
  border-radius: 16rpx;
  box-sizing: border-box;
  color: #fff;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 10rpx 0;
  padding: 50rpx 40rpx;
  position: relative;
}
.testpage .header .item {
  font-family: PingFangSC-Regular;
  margin-top: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.testpage .header :first-child {
  margin-top: 0;
}
.testpage .header .avatar {
  clip-path: circle(50%);
  height: 100rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  width: 100rpx;
}
.testpage .tip {
  color: #999;
  font-size: 20rpx;
  margin-bottom: 15rpx;
  margin-top: 10rpx;
}
.testpage .container .item {
  -ms-flex-pack: justify;
  -ms-flex-align: center;
  align-items: center;
  background-color: #fff;
  border-radius: 50rpx;
  box-shadow: 0 10rpx 28rpx -4rpx hsla(0, 0%, 89%, 0.65);
  color: #333;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 100rpx;
  justify-content: space-between;
  margin-top: 20rpx;
}
.testpage .container .item .icon {
  border-radius: 40rpx;
  font-size: 36rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 25rpx;
  text-align: center;
  width: 70rpx;
}
.testpage .container .item .operation {
  flex: auto;
  font-weight: 500;
  margin-left: 30rpx;
  text-align: left;
}
.testpage .container .item .right-row {
  color: #999;
  margin-right: 50rpx;
}
.testpage .container .item .switch {
  margin-right: 50rpx;
}
.testpage .container .item .icon-yellow {
  background-color: #fdf7dc;
  color: #edc447;
}
.testpage .container .item .icon-green {
  background-color: #e0faf6;
  color: #64e2c1;
}
.testpage .container .item .icon-blue {
  background-color: #dfe9f8;
  color: #326edc;
}
.testpage .container .item .icon-Orchid {
  background-color: #d8bfd8;
  color: orchid;
}
.testpage .container .item .icon-DarkSlateBlue {
  background-color: #d8bfd8;
  color: #483d8b;
}
.testpage .container .item .icon-share-pic {
  background-color: #e1ffff;
  color: green;
}
.testpage .env .env-item {
  -ms-flex-pack: justify;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 90rpx;
  justify-content: space-between;
  line-height: 90rpx;
  padding: 0 60rpx;
}
.testpage .env :first-child {
  border-radius: 20rpx 20rpx 0 0;
}
.testpage .env .env-item-checked,
.testpage .env .env-item-hover {
  background-color: #f9f9fa;
}
.testpage .env .ifont-selected {
  color: #0080ff;
}
.testpage .env .tip {
  color: #0080ff;
  font-size: 20rpx;
  padding: 0 60rpx 30rpx;
}
.testpage .dialog {
  -ms-flex-pack: justify;
  background-color: #fff;
  border-radius: 26rpx;
  box-sizing: border-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-direction: column;
  height: 380rpx;
  justify-content: space-between;
  left: 50%;
  padding: 40rpx;
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 600rpx;
  z-index: 1001;
}
.testpage .dialog .title {
  color: #333;
  font-size: 38rpx;
  height: 52rpx;
  line-height: 52rpx;
  margin: 0 auto;
  text-align: center;
  width: 500rpx;
}
.testpage .dialog .main {
  border: 1rpx solid #ccc;
  border-radius: 8rpx;
  font-size: 34rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 22rpx;
}
.testpage .dialog .footer {
  -ms-flex-pack: justify;
  -ms-flex-align: end;
  align-items: flex-end;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  justify-content: space-between;
}
.testpage .dialog .footer :nth-child(n) {
  background-color: #fff;
  border: 2rpx solid #d9d9d9;
  border-radius: 8rpx;
  box-shadow: 0 4rpx 0 rgba(0, 0, 0, 0.015);
  color: rgba(0, 0, 0, 0.65);
  font-size: 32rpx;
  font-weight: 400;
  height: 88rpx;
  line-height: 88rpx;
  margin: 0;
  padding: 0 30rpx;
  position: relative;
  text-align: center;
  touch-action: manipulation;
  transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  user-select: none;
  white-space: nowrap;
  width: 250rpx;
}
.testpage .dialog .footer .btn-cancel {
  color: #666;
}
.testpage .dialog .footer .btn-confirm {
  background-color: #0080ff;
  border-color: #0080ff;
  box-shadow: 0 4rpx 0 rgba(0, 0, 0, 0.045);
  color: #fff;
  text-shadow: 0-2rpx 0 rgba(0, 0, 0, 0.12);
}
.testpage .pop-mask {
  background: rgba(0, 0, 0, 0.7);
  bottom: 0;
  left: 0;
  opacity: 0;
  position: fixed;
  right: 0;
  top: 0;
  transition: opacity 0.25s ease-in-out;
  z-index: 960;
}
.testpage .pop-mask.active {
  opacity: 1;
}
.testpage .pop-botm {
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  bottom: 0;
  left: 0;
  position: fixed;
  right: 0;
  top: auto;
  transform: translate3d(0, 100%, 0);
  transition: all 0.25s ease-in-out;
  z-index: 1000;
}
.testpage .pop-botm.active {
  transform: translateZ(0);
}
.testpage .pop-botm-hd {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  font-size: 34rpx;
  height: 112rpx;
  line-height: 112rpx;
  text-align: center;
}
.testpage .pop-botm-hd .tit {
  flex: 1;
  font-size: 36rpx;
  font-weight: 600;
}
.testpage .pop-botm-hd .btn-cancel,
.testpage .pop-botm-hd .btn-submit {
  font-size: 32rpx;
  width: 144rpx;
}
.testpage .pop-botm-hd .btn-cancel {
  color: #666;
}
.testpage .pop-botm-hd .btn-submit {
  font-weight: 600;
}
.testpage .pop-botm-bd {
  border-top: 1rpx solid #eee;
  padding: 30rpx 0;
}
.testpage .pop-botm-bd .col {
  -ms-flex-align: center;
  align-items: center;
  color: #333;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  font-size: 30rpx;
  padding: 20rpx 30rpx;
}
.testpage .pop-botm-bd .col .lb {
  color: #333;
  font-weight: 500;
  margin-right: 20rpx;
  width: 100rpx;
}
.testpage .pop-botm-bd .col .lb.long {
  width: 250rpx;
}
.testpage .pop-botm-bd .col .btn {
  background: #0080ff;
  border-radius: 40rpx;
  color: #fff;
  font-size: 30rpx;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-left: auto;
  padding: 0 20rpx;
}
.testpage .zt-switch-container {
  margin-right: 25rpx;
}
