  position: absolute;
  right: -34rpx;
  top: 86rpx;
}
.hb-box .pic text.on:before {
  background: url(https://images3.c-ctrip.com/train/hd/20200414-flight-monitor/hb-icon.png)
    no-repeat;
  background-size: 100% 100%;
  content: "";
  height: 42rpx;
  position: absolute;
  right: -90rpx;
  top: 30rpx;
  width: 86rpx;
}
.hb-box .btn {
  background: url(https://images3.c-ctrip.com/train/hd/20200414-flight-monitor/hb-btn-bg.png)
    no-repeat;
  background-size: 100% 100%;
  bottom: 110rpx;
  box-sizing: border-box;
  color: #ed0745;
  font-size: 40rpx;
  font-weight: 700;
  height: 140rpx;
  left: 50%;
  line-height: 40rpx;
  margin-left: -249rpx;
  padding-top: 26rpx;
  position: absolute;
  width: 498rpx;
}
.logo {
  background: url(https://images3.c-ctrip.com/train/hd/20200414-flight-monitor/logo.png)
    no-repeat;
  background-size: 100% 100%;
}
.logo,
.logoty {
  height: 86rpx;
  left: 50%;
  margin-left: -131rpx;
  position: absolute;
  top: 88%;
  width: 262rpx;
}
.logoty {
  background: url(https://images3.c-ctrip.com/train/hd/20200414-flight-monitor/logoty.png)
    no-repeat;
  background-size: 100% 100%;
}
.animat {
  animation: mymove 1s infinite;
  animation-direction: alternate;
  animation-timing-function: ease-in-out;
  position: relative;
}
@-webkit-keyframes mymove {
  0% {
    transform: scale(1);
  }
  25% {
    transform: scale(0.9);
  }
  50% {
    transform: scale(1);
  }
  75% {
    transform: scale(0.9);
  }
}
@keyframes mymove {
  0% {
    transform: scale(1);
  }
  25% {
    transform: scale(0.9);
  }
  50% {
    transform: scale(1);
  }
  75% {
    transform: scale(0.9);
  }
}
