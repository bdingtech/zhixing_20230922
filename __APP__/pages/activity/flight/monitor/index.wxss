@charset "UTF-8";
.ClearBox:after,
.ClearBox:before {
  clear: both;
  content: " ";
  display: table;
}
.shadow {
  background: rgba(0, 0, 0, 0.7);
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 100;
}
text,
view {
  font: 400 28rpx/1 PingFang SC;
}
