page {
  background: #fff;
}
.cont {
  flex: 1;
}
.evaluate-view {
  -webkit-overflow-scrolling: touch;
  overflow-y: auto;
  position: relative;
}
.evaluate-tab {
  border-bottom: 1rpx solid #eee;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 84rpx;
}
.evaluate-tab .item {
  color: #999;
  flex: 1;
  font-size: 30rpx;
  line-height: 84rpx;
  text-align: center;
}
.evaluate-tab .item text {
  font-size: 26rpx;
}
.evaluate-tab .item.cur {
  font-weight: 600;
  position: relative;
}
.evaluate-tab .item.cur::after {
  border-radius: 10rpx;
  bottom: -2rpx;
  content: "";
  height: 5rpx;
  left: 50%;
  margin-left: -50rpx;
  position: absolute;
  width: 100rpx;
}
.evaluate-tab.zx .item.cur {
  color: #0080ff;
}
.evaluate-tab.zx .item.cur::after {
  background: #0080ff;
}
.evaluate-tab.ty .item.cur {
  color: #fc6e51;
}
.evaluate-tab.ty .item.cur::after {
  background: #fc6e51;
}
.evaluate-total {
  font-size: 32rpx;
  font-weight: 600;
  line-height: 60rpx;
  margin: 30rpx 40rpx 0;
}
.evaluate-total .strong {
  color: #ff7d00;
  font-size: 44rpx;
  margin: 0 14rpx;
}
.evaluate-total .iconfont {
  color: #ff7d00;
  font-size: 22rpx;
  font-weight: 400;
  margin: -4rpx 6rpx 0 0;
}
.evaluate-tag {
  margin: 0 10rpx 16rpx 30rpx;
}
.evaluate-tag-bd {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
}
.evaluate-tag-bd .item {
  border: 1rpx solid #eee;
  border-radius: 56rpx;
  font-size: 24rpx;
  line-height: 56rpx;
  margin: 20rpx 20rpx 0 0;
  padding: 0 24rpx;
}
.evaluate-tag-bd .item.bgcolor-primary {
  border: 0;
  color: #fff;
}
.evaluate-tag-more {
  border-bottom: 1rpx solid #eee;
  line-height: 74rpx;
  margin-right: 20rpx;
  text-align: center;
}
.evaluate-tag-more .ifont-arrdown2 {
  color: #666;
  font-size: 24rpx;
}
.evaluate-tag-more.open .ifont-arrdown2 {
  transform: rotate(180deg);
}
.evaluate-list {
  margin-left: 30rpx;
}
.evaluate-list .item {
  border-bottom: 1rpx solid #eee;
  padding: 30rpx 30rpx 40rpx 0;
}
.evaluate-list .item .open {
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 6rpx;
}
.evaluate-info {
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-bottom: 20rpx;
}
.evaluate-info .avatar {
  border-radius: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
  width: 80rpx;
}
.evaluate-info .name {
  color: #666;
  font-size: 26rpx;
  line-height: 36rpx;
  margin-bottom: 8rpx;
}
.evaluate-info .score {
  background: #fdf2e6;
  border-radius: 36rpx;
  color: #ff7d00;
  display: inline-block;
  font-size: 22rpx;
  font-weight: 600;
  line-height: 36rpx;
  padding: 0 16rpx 0 44rpx;
  position: relative;
}
.evaluate-info icon {
  background: url(https://pic.c-ctrip.com/train/zt/ticket/evaluate-face.png)
    no-repeat;
  background-size: 40rpx 200rpx;
  height: 40rpx;
  left: -2rpx;
  position: absolute;
  top: -2rpx;
  width: 40rpx;
}
.evaluate-info .icon-score-5 {
  background-position: 0 0;
}
.evaluate-info .icon-score-4 {
  background-position: 0-40rpx;
}
.evaluate-info .icon-score-3 {
  background-position: 0-80rpx;
}
.evaluate-info .icon-score-2 {
  background-position: 0-120rpx;
}
.evaluate-info .icon-score-1 {
  background-position: 0-160rpx;
}
.evaluate-info .rbox,
.evaluate-info .rboxNoBefore {
  color: #999;
  font-size: 24rpx;
  line-height: 38rpx;
  padding-left: 40rpx;
  position: relative;
  text-align: right;
  white-space: pre-wrap;
  z-index: 2;
}
.evaluate-info .rbox::before {
  background: url(https://pic.c-ctrip.com/train/zt/ticket/evaluate-elite.png)
    no-repeat;
  background-size: cover;
  bottom: 0;
  content: "";
  height: 80rpx;
  left: 0;
  position: absolute;
  width: 102rpx;
  z-index: -1;
}
.evaluate-txt {
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
}
.evaluate-txt.ellipsis {
  -webkit-line-clamp: 6;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
}
.evaluate-img {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
  margin-top: 12rpx;
  position: relative;
}
.evaluate-img .pic {
  border-radius: 6rpx;
  height: 162rpx;
  margin: 12rpx 12rpx 0 0;
  width: 162rpx;
}
.evaluate-img .pic:nth-child(4n) {
  margin-right: 0;
}
.evaluate-img .num {
  background: rgba(0, 0, 0, 0.7);
  border-radius: 36rpx;
  bottom: 8rpx;
  color: #fff;
  font-size: 22rpx;
  font-weight: 600;
  line-height: 36rpx;
  padding: 0 12rpx;
  position: absolute;
  right: 22rpx;
}
.evaluate-list .finish {
  color: #999;
  font-size: 26rpx;
  padding: 40rpx 0 70rpx;
  text-align: center;
}
.sketelonWrap {
  -webkit-overflow-scrolling: touch;
  overflow-y: auto;
  position: relative;
}
.skeleton-item {
  background: #fff;
  box-sizing: border-box;
  overflow: hidden;
}
.skeleton-item .scoreTitle {
  border-radius: 8rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-direction: column;
  height: 64rpx;
  width: 50%;
}
.skeleton-item .scoreTitle,
.skeleton-item .skeItem {
  animation: breathing 1.5s infinite;
  background: #f5f5f5;
}
.skeleton-item .skeItem {
  border-color: #f5f5f5;
  color: #f5f5f5;
}
.skeleton-item .iconLine {
  -ms-flex-pack: center;
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 74rpx;
  justify-content: center;
  width: 100%;
}
.skeleton-item .evaItemWrap {
  border-radius: 10rpx;
  width: 100%;
}
.skeleton-item .evaluate-info {
  -ms-flex-align: center;
  -ms-flex-pack: justify;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 80rpx;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.skeleton-item .head {
  animation: breathing 1.5s infinite;
  background: #f5f5f5;
  border-radius: 8rpx;
  height: 100%;
  width: 26%;
}
.skeleton-item .cont {
  margin-top: 20rpx;
  overflow: hidden;
}
.skeleton-item .textLineWrap {
  -ms-flex-align: center;
  align-items: center;
  border-radius: 8rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 40rpx;
}
.skeleton-item .textLine {
  animation: breathing 1.5s infinite;
  background: #f5f5f5;
  border-radius: 8rpx;
  height: 30rpx;
  width: 100%;
}
.skeleton-item .fold {
  flex: 1;
  height: 40rpx;
  margin-top: 6rpx;
}
.skeleton-item .imgWrap {
  -ms-flex-pack: justify;
  -ms-flex-align: space-between;
  align-items: space-between;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
  height: 348rpx;
  justify-content: space-between;
  margin-top: 12rpx;
}
.skeleton-item .img {
  animation: breathing 1.5s infinite;
  background: #f5f5f5;
  border-radius: 8rpx;
  height: 162rpx;
  width: 162rpx;
}
@keyframes breathing {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
  100% {
    opacity: 1;
  }
}
@-webkit-keyframes breathing {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
  100% {
    opacity: 1;
  }
}
.stickBtn {
  -ms-flex-pack: center;
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  border-radius: 50%;
  bottom: 160rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
  opacity: 0.9;
  position: fixed;
  right: 30rpx;
}
.stickBtn,
.stickImg {
  height: 100rpx;
  width: 100rpx;
}
