@charset "UTF-8";
@import "/pages/hotel/sub-vendors.wxss";
.wrap {
  padding-bottom: 112rpx;
}
.kq-tit {
  font-size: 30rpx;
  font-weight: 600;
  line-height: 42rpx;
  margin: 40rpx 30rpx 20rpx;
}
.kq-tit .ifont-quan {
  font-size: 38rpx;
  font-weight: 400;
  margin: -6rpx 12rpx 0 0;
}
.kq-coupon .item {
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  border-radius: 20rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 180rpx;
  margin: 20rpx;
  position: relative;
}
.kq-coupon .item::after,
.kq-coupon .item::before {
  background: #efefef;
  border-radius: 32rpx;
  content: "";
  height: 32rpx;
  margin-top: -16rpx;
  position: absolute;
  top: 50%;
  width: 32rpx;
}
.kq-coupon .item::before {
  left: -16rpx;
}
.kq-coupon .item::after {
  right: -16rpx;
}
.kq-coupon .item .price {
  font-size: 68rpx;
  font-weight: 600;
  position: relative;
  text-align: center;
  width: 188rpx;
}
.kq-coupon .item .price::before {
  content: "ï¿¥";
  font-size: 26rpx;
}
.kq-coupon .item .cont {
  flex: 1;
}
.kq-coupon .item .tit {
  font-size: 30rpx;
  font-weight: 600;
  line-height: 42rpx;
  margin-bottom: 14rpx;
}
.kq-coupon .item .ifont-detail {
  color: #999;
  font-size: 30rpx;
  font-weight: 400;
  margin: -4rpx 0 0 12rpx;
}
.kq-coupon .item .txt {
  color: #999;
  font-size: 22rpx;
  line-height: 32rpx;
}
.kq-coupon .item .no-coupon {
  flex: 1;
  font-size: 30rpx;
  text-align: center;
}
.kq-coupon .item .btn-used {
  border-radius: 56rpx;
  font-size: 24rpx;
  font-weight: 600;
  height: 56rpx;
  line-height: 56rpx;
  margin-right: 40rpx;
  padding: 0;
  width: 136rpx;
}
.container {
  -ms-flex-align: center;
  -ms-flex-pack: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
}
.nouse {
  border-radius: 15rpx;
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  width: 500rpx;
}
.nouse.ty {
  border: 2rpx solid #fc6e51;
  color: #fc6e51;
}
.nouse.zx {
  border: 2rpx solid #0080ff;
  color: #0080ff;
}
