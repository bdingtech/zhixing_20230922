@charset "UTF-8";
@import "/pages/hotel/sub-vendors.wxss";
.cont {
  flex: 1;
}
.monitor-top {
  padding: 24rpx 24rpx 0;
  position: relative;
}
.monitor-top::before {
  content: "";
  height: 160rpx;
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
}
.monitor-top.zx::before {
  background: -webkit-linear-gradient(top, #0066e6, #0080ff);
}
.monitor-top.ty::before {
  background: #3c4365;
}
.monitor-top::after {
  background: url(https://pic.c-ctrip.com/train/zt/wechat/arc-mask.png)
    no-repeat;
  background-size: cover;
  content: "";
  height: 80rpx;
  left: 0;
  position: absolute;
  top: 80rpx;
  width: 750rpx;
}
.monitor-top-bd {
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 0 0 36rpx rgba(0, 0, 0, 0.08);
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 44rpx 30rpx 36rpx;
  position: relative;
  z-index: 2;
}
.monitor-top-bd .lbox {
  height: 60rpx;
  margin-right: 16rpx;
  overflow: hidden;
  position: relative;
  width: 60rpx;
}
.monitor-top-bd .icon-loading {
  animation: loading 1s linear 0s infinite;
  background-repeat: no-repeat;
  background-size: cover;
  height: 60rpx;
  width: 60rpx;
}
@-webkit-keyframes loading {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(1turn);
  }
}
@keyframes loading {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(1turn);
  }
}
.monitor-top-bd .icon-loading.zx {
  background-image: url(https://pic.c-ctrip.com/train/zt/hotel/loading-zx.png);
}
.monitor-top-bd .icon-loading.ty {
  background-image: url(https://pic.c-ctrip.com/train/zt/hotel/loading-ty.png);
}
.monitor-top-bd .ifont-light {
  font-size: 24rpx;
  left: 50%;
  margin-top: 2rpx;
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
}
.monitor-top-bd .tit {
  font-size: 40rpx;
  font-weight: 600;
  line-height: 56rpx;
  margin-bottom: 14rpx;
  margin-top: 2rpx;
}
.monitor-top-bd .txt {
  font-size: 30rpx;
  line-height: 42rpx;
}
.monitor-top-bd .tgrey {
  font-size: 24rpx;
  margin-right: 24rpx;
}
.monitor-top-bd .btn-edit {
  background: #f5f5f5;
  border-radius: 48rpx;
  font-size: 24rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin: 6rpx 0 0 30rpx;
  padding: 0;
  width: 96rpx;
}
.wrap {
  margin: 0 24rpx;
  padding-bottom: 100rpx;
}
.monitor-tit {
  font-size: 36rpx;
  font-weight: 600;
  line-height: 50rpx;
  margin: 40rpx 6rpx 20rpx;
}
.monitor-tag {
  margin: 0 6rpx;
  position: relative;
}
.monitor-tag::after {
  background: -webkit-linear-gradient(left, hsla(0, 0%, 94%, 0), #efefef);
  bottom: 0;
  content: "";
  height: 100rpx;
  position: absolute;
  right: 0;
  width: 40rpx;
  z-index: 5;
}
.monitor-tag .tag-list {
  -webkit-overflow-scrolling: touch;
  overflow-x: auto;
  padding-bottom: 20rpx;
  white-space: nowrap;
}
.monitor-tag .tag-list .item {
  background: #fff;
  border-radius: 64rpx;
  color: #666;
  display: inline-block;
  font-size: 24rpx;
  line-height: 64rpx;
  margin-right: 20rpx;
  padding: 0 24rpx;
}
.monitor-tag .tag-list .zx {
  background: #0382ff;
  color: #fff;
  font-weight: 600;
}
.monitor-tag .tag-list .ty {
  background: #fc6e51;
  color: #fff;
  font-weight: 600;
}
.skeleton-item {
  background: #fff;
  border-radius: 16rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-bottom: 20rpx;
  min-height: 136rpx;
  padding: 30rpx 150rpx 30rpx 180rpx;
  position: relative;
}
.skeleton-item .img {
  animation: breathing 1.2s infinite;
  background: #f5f5f5;
  border-radius: 6rpx;
  border-radius: 4rpx;
  bottom: 30rpx;
  height: auto;
  left: 30rpx;
  position: absolute;
  top: 30rpx;
  width: 120rpx;
}
.skeleton-item .s-tit {
  height: 60rpx;
}
.skeleton-item .s-tit,
.skeleton-item .s-txt {
  animation: breathing 1.2s infinite;
  background: #f5f5f5;
  border-radius: 4rpx;
  width: 430rpx;
}
.skeleton-item .s-txt {
  height: 34rpx;
  margin-top: 35rpx;
}
.monitor-list {
  overflow-x: hidden;
  width: 100%;
}
.monitor-list .item {
  background: #fff;
  border-radius: 16rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-bottom: 20rpx;
  min-height: 136rpx;
  padding: 30rpx 150rpx 30rpx 180rpx;
  position: relative;
}
.monitor-list .img {
  border-radius: 6rpx;
  bottom: 30rpx;
  height: auto;
  left: 30rpx;
  position: absolute;
  top: 30rpx;
  width: 120rpx;
}
.monitor-list .tit {
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  font-size: 32rpx;
  font-weight: 600;
  line-height: 44rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}
.monitor-list .txt {
  color: #666;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-top: 2rpx;
}
.monitor-list .price {
  font-size: 40rpx;
  font-weight: 600;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
}
.monitor-list .price::before {
  content: "Â¥";
  font-size: 26rpx;
  margin-right: 2rpx;
}
.monitor-list .rbox {
  bottom: 30rpx;
  position: absolute;
  right: 30rpx;
  text-align: right;
}
.monitor-list .rbox .txt {
  font-size: 22rpx;
}
.monitor-list .tag-grey,
.monitor-list .tag-red {
  border-radius: 4rpx;
  font-size: 22rpx;
  line-height: 34rpx;
  margin-top: 6rpx;
  padding: 0 8rpx;
}
.monitor-list .tag-red {
  border: 1rpx solid rgba(253, 77, 33, 0.5);
}
.monitor-list .tag-grey {
  border: 1rpx solid #e1e1e1;
  color: #666;
}
.monitor-list .tag-red .iconfont {
  font-size: 18rpx;
  margin: -4rpx 2rpx 0 0;
}
.monitor-more {
  font-size: 26rpx;
  line-height: 36rpx;
  margin-top: 30rpx;
  text-align: center;
}
.monitor-more .ifont-arr {
  font-size: 20rpx;
  margin: -4rpx 0 0 4rpx;
}
.btn-cancel {
  background: #e8e8e8;
  border-radius: 96rpx;
  font-size: 36rpx;
  height: 96rpx;
  line-height: 96rpx;
  margin-top: 50rpx;
}
.item-delete {
  -ms-flex-align: center;
  -ms-flex-pack: center;
  align-items: center;
  background: transparent;
  bottom: 0;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: absolute;
  right: -135rpx;
  top: 0;
  width: 160rpx;
}
.item-delete .ifont-trash {
  background: #fff;
  border-radius: 80rpx;
  color: #666;
  font-size: 34rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 18rpx;
  text-align: center;
  width: 80rpx;
}
.pop-mid {
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 0 10rpx 20rpx -4rpx rgba(43, 49, 61, 0.1);
  box-sizing: border-box;
  left: 50%;
  opacity: 0;
  position: fixed;
  top: 50%;
  transform: translate(-50%, -50%);
  z-index: 1000;
}
.pop-mid.active {
  opacity: 1;
}
.loading {
  -ms-flex-pack: center;
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 150rpx;
  justify-content: center;
  margin: 0 auto;
  width: 150rpx;
  z-index: 999;
}
.loading text {
  -webkit-animation: load 1s ease infinite;
  background: #90ee90;
  border-radius: 4rpx;
  display: inline-block;
  height: 55rpx;
  margin: 0 2rpx;
  width: 8rpx;
}
@keyframes breathing {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
  100% {
    opacity: 1;
  }
}
@-webkit-keyframes breathing {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
  100% {
    opacity: 1;
  }
}
@-webkit-keyframes load {
  0%,
  100% {
    background: #90ee90;
    height: 40rpx;
  }
  50% {
    background: #add8e6;
    height: 70rpx;
    margin: -15rpx 2rpx;
  }
}
.loading text:nth-child(2) {
  -webkit-animation-delay: 0.2s;
}
.loading text:nth-child(3) {
  -webkit-animation-delay: 0.4s;
}
.loading text:nth-child(4) {
  -webkit-animation-delay: 0.6s;
}
.loading text:nth-child(5) {
  -webkit-animation-delay: 0.8s;
}
