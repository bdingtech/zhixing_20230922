@charset "UTF-8";
@import "/pages/hotel/sub-vendors.wxss";
.hotel-dp-top {
  background: #fff;
  margin-bottom: 16rpx;
  padding: 24rpx 30rpx 40rpx;
}
.hotel-dp-value {
  display: table;
  width: 100%;
}
.hotel-dp-value .item {
  display: table-cell;
  text-align: center;
  vertical-align: bottom;
}
.hotel-dp-value .item .strong {
  color: #333;
  font-size: 34rpx;
  font-weight: 600;
  line-height: 48rpx;
}
.hotel-dp-value .item .txt {
  color: #999;
  font-size: 24rpx;
}
.hotel-dp-value .item.general {
  padding-right: 60rpx;
  width: 180rpx;
}
.hotel-dp-value .item.general .strong {
  font-size: 88rpx;
  line-height: 124rpx;
}
.hotel-dp-value .item.general .txt {
  margin-top: -10rpx;
}
.hotel-dp-key {
  margin-top: 40rpx;
  overflow: hidden;
}
.hotel-dp-key .item {
  background: #f8f8f8;
  border-radius: 64rpx;
  box-sizing: border-box;
  color: #666;
  float: left;
  font-size: 26rpx;
  line-height: 64rpx;
  margin: 0 20rpx 24rpx 0;
  min-width: 120rpx;
  padding: 0 28rpx;
  text-align: center;
}
.hotel-dp-key.zx .item.cur {
  background: #f3f7ff;
  color: #0080ff;
}
.hotel-dp-key.ty .item.cur {
  background: rgba(252, 110, 81, 0.15);
  color: #fc6e51;
}
.hotel-dp-top .key-more {
  font-size: 26rpx;
  margin-top: 6rpx;
  text-align: center;
}
.hotel-dp-box {
  background: #fff;
  margin-bottom: 16rpx;
}
.hotel-dp-box .tit {
  color: #333;
  font-size: 36rpx;
  font-weight: 600;
  padding: 30rpx 30rpx 0;
}
.hotel-dp-box .item {
  border-bottom: 1rpx solid #eee;
  padding: 30rpx;
}
.hotel-dp-box .item:last-child {
  border-bottom: 0;
}
.hotel-dp-hd {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  position: relative;
}
.hotel-dp-hd .ip-addr {
  color: #999;
  font-size: 22rpx;
  line-height: 30rpx;
}
.hotel-dp-hd .pic-avatar {
  border-radius: 80rpx;
  height: 80rpx;
  margin-right: 16rpx;
  width: 80rpx;
}
.hotel-dp-hd .cont {
  flex: 1;
}
.hotel-dp-hd .cont .evaluate {
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-wrap: row wrap;
  font-size: 24rpx;
  line-height: 32rpx;
  margin-top: 6rpx;
}
.hotel-dp-hd .cont .evaluate .color-primary {
  margin-right: 10rpx;
}
.hotel-dp-hd .grade {
  font-size: 36rpx;
  font-weight: 600;
  line-height: 50rpx;
  padding-top: 2rpx;
  position: absolute;
  right: 0;
  text-align: right;
  top: 0;
}
.hotel-dp-hd .name {
  font-size: 32rpx;
  font-weight: 600;
  line-height: 44rpx;
}
.hotel-dp-hd .tag {
  border: 1rpx solid rgba(251, 75, 75, 0.4);
  border-radius: 4rpx;
  display: inline-block;
  font-size: 20rpx;
  line-height: 30rpx;
  margin: -4rpx 0 0 16rpx;
  padding: 0 4rpx;
  vertical-align: middle;
}
.hotel-dp-date {
  color: #999;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-top: 26rpx;
}
.hotel-dp-date .line {
  background: #d8d8d8;
  display: inline-block;
  height: 24rpx;
  line-height: 34rpx;
  position: relative;
  top: 4rpx;
  width: 1rpx;
}
.hotel-dp-date text {
  margin-left: 14rpx;
}
.hotel-dp-reply,
.hotel-dp-txt {
  margin-top: 30rpx;
}
.hotel-dp-reply {
  background: #f8f8f8;
  border-radius: 16rpx;
  color: #999;
  padding: 24rpx;
}
.hotel-dp-reply .reply-head {
  -ms-flex-align: center;
  -ms-flex-pack: justify;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  font-size: 26rpx;
  justify-content: space-between;
  margin-bottom: 8rpx;
}
.hotel-dp-reply .reply-head .reply-place {
  font-size: 22rpx;
}
.hotel-dp-reply .txt,
.hotel-dp-txt .txt {
  font-size: 26rpx;
  line-height: 40rpx;
}
.hotel-dp-reply .txt.ellipsis,
.hotel-dp-txt .txt.ellipsis {
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: normal;
}
.hotel-dp-reply .more,
.hotel-dp-txt .more {
  font-size: 28rpx;
  margin-top: 8rpx;
  text-align: right;
}
.hotel-dp-pic {
  margin-top: 10rpx;
  overflow: hidden;
}
.hotel-dp-pic .pic-dp {
  border-radius: 4rpx;
  float: left;
  height: 164rpx;
  margin: 11rpx 11rpx 0 0;
  width: 164rpx;
}
.hotel-dp-pic .pic-dp:nth-child(4n) {
  margin-right: 0;
}
.hotel-qa-cont .t .info {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.hotel-qa-cont .t .info .line {
  background-color: #ccc;
  display: block;
  margin: 6rpx 8rpx;
  width: 2rpx;
}
.top-bar {
  background: #fff;
  height: 88rpx;
  padding: 40rpx 210rpx 0;
  position: relative;
}
.top-bar .ifont-back {
  bottom: 0;
  font-size: 36rpx;
  height: 60rpx;
  left: 0;
  line-height: 60rpx;
  margin-top: 10rpx;
  position: absolute;
  text-align: center;
  width: 60rpx;
}
.top-tab {
  background: #f0f0f0;
  border-radius: 60rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 60rpx;
  margin-top: 10rpx;
}
.top-tab .item {
  border-radius: 60rpx;
  flex: 1;
  font-size: 30rpx;
  font-weight: 600;
  line-height: 60rpx;
  text-align: center;
}
.top-tab .bgcolor-primary {
  color: #fff;
}
.wrap {
  -webkit-overflow-scrolling: touch;
  flex: 1;
  overflow-y: auto;
  padding-bottom: 260rpx;
}
.white-box {
  background: #fff;
}
.hotel-qa-tag {
  -webkit-overflow-scrolling: touch;
  border-bottom: 1rpx solid #eee;
  margin: 0 30rpx;
  overflow-x: auto;
  padding: 32rpx 0;
  white-space: nowrap;
}
.hotel-qa-tag .item:last-child {
  margin-right: 0;
}
.hotel-qa-tag .item text {
  font-size: 20rpx;
}
.hotel-qa-tag .item.ty,
.hotel-qa-tag .item.zx {
  color: #fff;
}
.hotel-qa-tag .item.ty {
  background: #fc6e51;
}
.hotel-qa-tag .item.zx {
  background: #0080ff;
}
.hotel-qa-cont {
  margin: 0 30rpx;
}
.hotel-qa-cont .item {
  border-bottom: 1rpx solid #eee;
  padding: 48rpx 0 48rpx 52rpx;
}
.hotel-qa-cont .item:last-child {
  border-bottom: 0;
}
.hotel-qa-cont .q {
  font-size: 32rpx;
  font-weight: 600;
  line-height: 40rpx;
  position: relative;
}
.hotel-qa-cont .a::before,
.hotel-qa-cont .q::before {
  border-radius: 4rpx;
  color: #fff;
  font-size: 24rpx;
  height: 36rpx;
  left: -52rpx;
  line-height: 36rpx;
  position: absolute;
  text-align: center;
  width: 36rpx;
}
.hotel-qa-cont .q::before {
  content: "问";
  top: 2rpx;
}
.hotel-qa-cont.zx .q::before {
  background: #0080ff;
}
.hotel-qa-cont.ty .q::before {
  background: #5864a9;
}
.hotel-qa-cont .ip-addr {
  -ms-flex-align: center;
  -ms-flex-pack: end;
  align-items: center;
  color: #999;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
  font-size: 22rpx;
  justify-content: flex-end;
  margin-top: 10rpx;
}
.hotel-qa-cont .a {
  font-size: 28rpx;
  line-height: 36rpx;
  margin-top: 32rpx;
  position: relative;
}
.hotel-qa-cont .a::before {
  content: "答";
  top: 0;
}
.hotel-qa-cont.zx .a::before {
  background: #fe4839;
}
.hotel-qa-cont.ty .a::before {
  background: #fc6e51;
}
.hotel-qa-cont .t {
  -ms-flex-align: center;
  -ms-flex-pack: justify;
  align-items: center;
  color: #999;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
  font-size: 22rpx;
  justify-content: space-between;
  margin-top: 10rpx;
}
.hotel-qa-cont .none {
  color: #999;
  font-size: 28rpx;
  margin-top: 24rpx;
}
.hotel-qa-cont .more {
  font-size: 26rpx;
  margin-top: 24rpx;
}
.botm-fixed {
  background: #fff;
  bottom: 0;
  box-shadow: 0-4rpx 10rpx rgba(0, 0, 0, 0.06);
  padding: 16rpx 20rpx;
  position: fixed;
  width: 95%;
  z-index: 10;
}
.botm-fixed .btn-primary {
  border-radius: 176rpx;
  font-size: 36rpx;
  height: 88rpx;
  line-height: 88rpx;
}
