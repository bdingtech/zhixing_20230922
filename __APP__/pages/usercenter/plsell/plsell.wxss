@charset "UTF-8";
@import "/pages/usercenter/sub-vendors.wxss";
page {
  background: #f5f5f5;
}
.cont {
  flex: 1;
}
.wrap {
  bottom: 0;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-direction: column;
  left: 0;
  position: absolute;
  right: 0;
  top: 108rpx;
}
.wrap::before {
  background: #553aa9;
  height: 96rpx;
}
.wrap::after,
.wrap::before {
  content: "";
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
}
.wrap::after {
  background: -webkit-linear-gradient(top, #fff, #f5f5f5);
  border-radius: 32rpx 32rpx 0 0;
  height: 200rpx;
  z-index: 3;
}
.header {
  background-color: #553aaa;
  height: 108rpx;
  position: relative;
}
.header .tabs {
  -ms-flex-pack: space-evenly;
  height: 88rpx;
  justify-content: space-evenly;
}
.header .tabs,
.header .tabs .tab {
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.header .tabs .tab {
  -ms-flex-pack: center;
  font-family: PingFangSC-Medium;
  justify-content: center;
}
.header .tabs .tab .title {
  color: #d98c21;
  font-size: 42rpx;
  font-weight: 500;
  margin-right: 6rpx;
  opacity: 0.6;
}
.header .tabs .tab .sub-title {
  background: -webkit-linear-gradient(313.94deg, #f2c261, #d98c20);
  background: -o-linear-gradient(313.94deg, #f2c261 0, #d98c20 100%);
  background: linear-gradient(-223.94deg, #f2c261, #d98c20);
  border-radius: 6rpx;
  color: #23232a;
  font-size: 20rpx;
  height: 34rpx;
  line-height: 34rpx;
  opacity: 0.6;
  text-align: center;
  width: 92rpx;
}
.header .tabs .selected .sub-title,
.header .tabs .selected .title {
  opacity: 1;
}
.header .arrow {
  bottom: 0;
  height: 20rpx;
  left: 0;
  margin-left: calc(16.66667% - 532rpx / 6);
  position: absolute;
  transition: -webkit-transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
  width: calc(33.33333% + 532rpx / 6);
  z-index: 1;
}
.header .arrow::after {
  background: url(https://images3.c-ctrip.com/ztrip/train_bin/8yue/xcx/huiyuanshoumai/home_icon_triangle@3x.png)
    no-repeat;
  background-size: 78rpx 20rpx;
  bottom: 0;
  content: "";
  height: 18rpx;
  left: calc(50% - 38rpx);
  position: absolute;
  width: 78rpx;
}
.header .transform {
  transform: translateX(100%);
}
.main-box {
  -webkit-overflow-scrolling: touch;
  border-radius: 32rpx 32rpx 0 0;
  flex: 1;
  overflow-y: auto;
  padding: 16rpx 20rpx;
  position: relative;
  z-index: 5;
}
.white-box {
  background: #fff;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.white-box .htit {
  font-size: 36rpx;
  font-weight: 600;
  line-height: 44rpx;
}
.notice-box {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  font-size: 24rpx;
  line-height: 34rpx;
  padding: 26rpx 30rpx;
}
.notice-box .ifont-notices {
  font-size: 32rpx;
  margin-right: 10rpx;
}
.gift-equity,
.level-equity,
.select-member {
  padding: 30rpx;
}
.select-info {
  color: #666;
  font-size: 32rpx;
  line-height: 44rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}
.select-member-bd {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
  margin-top: 26rpx;
}
.select-member .item {
  border: 3rpx solid #ebebeb;
  border-radius: 16rpx;
  box-sizing: border-box;
  height: 208rpx;
  margin-right: 16rpx;
  padding: 24rpx 10rpx;
  position: relative;
  text-align: center;
  width: 206rpx;
}
.select-member .item:nth-child(3n) {
  margin-right: 0;
}
.select-member .tag {
  background: -webkit-linear-gradient(left, #f73, #f33);
  border-radius: 16rpx 0;
  color: #fff;
  font-size: 22rpx;
  left: -3rpx;
  line-height: 34rpx;
  padding: 0 16rpx;
  position: absolute;
  top: -10rpx;
}
.select-member .tit {
  font-size: 32rpx;
  font-weight: 600;
  line-height: 44rpx;
}
.select-member .price {
  font-size: 56rpx;
  font-weight: 500;
  height: 76rpx;
  line-height: 76rpx;
  overflow: hidden;
}
.select-member .price::before {
  content: "Â¥";
  font-size: 32rpx;
  margin-right: 4rpx;
}
.select-member .txt {
  color: #999;
  font-size: 22rpx;
}
.select-member .item.cur {
  background: #f3f0ff;
  border-color: #553aa9;
}
.hot-equity {
  padding-bottom: 40rpx;
}
.hot-equity .htit {
  -ms-flex-pack: justify;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 0;
}
.hot-equity .sub-txt {
  color: #666;
  font-size: 28rpx;
}
.hot-equity .content-l {
  flex-wrap: wrap;
  width: 100%;
}
.hot-equity .content-l,
.hot-equity .item {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.hot-equity .item {
  -ms-flex-pack: center;
  -ms-flex-align: center;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  width: 20%;
}
.hot-equity .item .icon {
  height: 80rpx;
  margin-top: 36rpx;
  width: 80rpx;
}
.hot-equity .item .name {
  color: #333;
  font-size: 22rpx;
  font-weight: 600;
  line-height: 32rpx;
  margin-top: 12rpx;
}
.hot-equity .item .desc {
  color: #999;
  font-size: 20rpx;
  line-height: 28rpx;
}
.gift-equity-bd,
.level-equity-bd {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-flow: row wrap;
  margin-top: 6rpx;
}
.gift-equity-bd .item {
  margin: 24rpx 12rpx 0 0;
  text-align: center;
  width: 208rpx;
}
.gift-equity-bd .item:nth-child(3n) {
  margin-right: 0;
}
.gift-equity-bd .img {
  display: block;
  height: 80rpx;
  margin: 0 auto;
  width: 80rpx;
}
.gift-equity-bd .tit {
  font-size: 26rpx;
  font-weight: 600;
  margin-top: 12rpx;
}
.gift-equity-bd .txt {
  color: #999;
  font-size: 22rpx;
  margin-top: 2rpx;
}
.level-equity-bd .item {
  margin: 24rpx 30rpx 0 0;
  text-align: center;
  width: 140rpx;
}
.level-equity-bd .item:nth-child(4n) {
  margin-right: 0;
}
.level-equity-bd .img {
  display: block;
  height: 80rpx;
  margin: 0 auto;
  width: 80rpx;
}
.level-equity-bd .tit {
  font-size: 24rpx;
  margin-top: 8rpx;
}
.botm-fixed {
  background: #fff;
  box-shadow: 0-4rpx 32rpx rgba(0, 0, 0, 0.05);
  padding: 20rpx 20rpx calc(20rpx + env(safe-area-inset-bottom));
}
.botm-fixed .btn-submit {
  border-radius: 88rpx;
  font-size: 36rpx;
  font-weight: 600;
  height: 88rpx;
  line-height: 88rpx;
}
.buy-intro-pop {
  background: #fff;
  border-radius: 16rpx;
  box-sizing: border-box;
  height: 780rpx;
  left: 50%;
  padding: 20rpx;
  position: fixed;
  top: 50%;
  transform: translate(-50%, -55%);
  width: 630rpx;
  z-index: 3001;
}
.buy-intro-pop::after {
  background: -webkit-gradient(
    linear,
    left top,
    left bottom,
    from(hsla(0, 0%, 100%, 0)),
    color-stop(50%, #fff)
  );
  background: -webkit-linear-gradient(top, hsla(0, 0%, 100%, 0), #fff 50%);
  background: -o-linear-gradient(top, hsla(0, 0%, 100%, 0), #fff 50%);
  background: linear-gradient(180deg, hsla(0, 0%, 100%, 0), #fff 50%);
  border-radius: 0 0 20rpx 20rpx;
  bottom: 0;
  content: "";
  height: 100rpx;
  left: 0;
  position: absolute;
  right: 0;
}
.buy-intro-pop .title {
  font-size: 36rpx;
  line-height: 36rpx;
  margin: 20rpx 0;
  text-align: center;
}
.buy-intro-pop .text {
  margin: 10rpx 0;
}
.buy-intro-pop .content {
  -webkit-overflow-scrolling: touch;
  color: #666;
  font-size: 28rpx;
  height: 600rpx;
  overflow-y: auto;
  padding: 0 20rpx 60rpx;
}
.icon-closed {
  bottom: -120rpx;
  height: 64rpx;
  left: 50%;
  position: absolute;
  transform: translateX(-50%);
  width: 64rpx;
}
.icon-closed::after,
.icon-closed::before {
  background: #fff;
  content: "";
  height: 4rpx;
  left: 50%;
  margin: -2rpx 0 0-25rpx;
  position: absolute;
  top: 50%;
  width: 50rpx;
}
.icon-closed::before {
  transform: rotate(45deg);
}
.icon-closed::after {
  transform: rotate(-45deg);
}
