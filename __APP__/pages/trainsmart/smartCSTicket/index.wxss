  align-items: center;
  background: #fee;
  border-radius: 20rpx 20rpx 0 0;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  left: 0;
  margin: -20rpx 20rpx 0;
  padding: 20rpx 30rpx 48rpx;
  position: relative;
  top: 20rpx;
}
.wrap-tips .wrap-tips-img {
  height: 36rpx;
  margin-right: 20rpx;
  width: 36rpx;
}
.wrap-tips .wrap-tips-txt {
  color: #fc6e51;
  flex: 1;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 36rpx;
  white-space: normal;
}
.reschedule-card {
  padding: 24rpx 20rpx 20rpx;
}
.reschedule-card .top-title {
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 48rpx;
  margin-bottom: 20rpx;
}
.reschedule-card .top-title .img {
  height: 40rpx;
  margin-right: 8rpx;
  width: 40rpx;
}
.reschedule-card.zx .top-title .img {
  background: url(https://images3.c-ctrip.com/ztrip/train_xie/2021-07/icon_endorse.png)
    no-repeat;
  background-size: cover;
}
.reschedule-card.ty .top-title .img {
  background: url(https://images3.c-ctrip.com/ztrip/train_xie/2021-07/icon_endorse_t.png)
    no-repeat;
  background-size: cover;
}
.reschedule-card .top-title .txt {
  font-size: 36rpx;
  font-weight: 600;
  height: 48rpx;
  line-height: 48rpx;
}
.reschedule-card .box-card {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.reschedule-card .box-card .left {
  background: #fff8f0;
}
.reschedule-card .box-card .left,
.reschedule-card .box-card .right {
  border-radius: 12rpx;
  flex: 1;
  padding: 20rpx 20rpx 24rpx;
  width: 303rpx;
}
.reschedule-card .box-card .right {
  background: #f1f8ff;
}
.reschedule-card .box-card .bar-txt {
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 44rpx;
  line-height: 44rpx;
  margin-bottom: 10rpx;
}
.reschedule-card .box-card .bar-txt .tag {
  background: #ff5959;
  border-radius: 6rpx;
  color: #fff;
  font-size: 20rpx;
  font-weight: 400;
  height: 30rpx;
  line-height: 30rpx;
  margin-left: 6rpx;
  padding: 0 6rpx;
}
.reschedule-card .box-card .bar-txt .title {
  font-size: 32rpx;
  font-weight: 500;
}
.reschedule-card .box-card .list {
  height: 34rpx;
  line-height: 34rpx;
  overflow: hidden;
  white-space: nowrap;
}
.reschedule-card .box-card .txt {
  color: #666;
  font-size: 24rpx;
}
.reschedule-card .box-card .origin-color {
  color: #ff5959;
}
.reschedule-card .box-card .new-color {
  color: #0080ff;
}
.reschedule-card .box-card .middal {
  background-image: -webkit-gradient(
    linear,
    left top,
    left bottom,
    from(#f5f7fa),
    to(rgba(239, 247, 255, 0))
  );
  background-image: -webkit-linear-gradient(
    top,
    #f5f7fa,
    rgba(239, 247, 255, 0)
  );
  background-image: -o-linear-gradient(
    top,
    #f5f7fa 0,
    rgba(239, 247, 255, 0) 100%
  );
  background-image: linear-gradient(180deg, #f5f7fa, rgba(239, 247, 255, 0));
  border-radius: 12rpx;
  margin: 0 4rpx;
  width: 56rpx;
}
.reschedule-card .box-card .middal .midal-arrow {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-bottom: 8rpx;
  margin-top: 60rpx;
  padding-left: 15rpx;
}
.reschedule-card .box-card .middal .midal-arrow-item-left {
  opacity: 0.4;
}
.reschedule-card .box-card .middal .midal-arrow-item-left,
.reschedule-card .box-card .middal .midal-arrow-item-right {
  border: 8rpx solid transparent;
  border-bottom-color: #999;
  height: 0;
  transform: rotate(90deg);
  width: 0;
}
.reschedule-card .box-card .middal .midal-text {
  color: #999;
  font-size: 20rpx;
  text-align: center;
}
.new-series {
  padding-bottom: calc(176rpx + env(safe-area-inset-bottom));
}
.wrap-v2 {
  margin-top: -134rpx;
  position: relative;
}
.wrap-v2.mt-v1 {
  margin-top: -290rpx;
}
.wrap-v2 .logo-box {
  text-align: center;
}
.wrap-v2 .logo-box .img-logo {
  height: 74rpx;
  margin-top: 40rpx;
  width: 458rpx;
}
.fixed-botm-v2 {
  background: #fff;
  border-radius: 40rpx 40rpx 0 0;
  bottom: 0;
  left: 0;
  padding: 16rpx 16rpx calc(16rpx + env(safe-area-inset-bottom));
  position: fixed;
  right: 0;
  z-index: 100;
}
.fixed-botm-v2,
.fixed-botm-v2 .h5-button {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.fixed-botm-v2 .h5-button {
  -ms-flex-align: center;
  -ms-flex-pack: center;
  align-items: center;
  border-radius: 24rpx;
  flex: 1;
  flex-direction: column;
  font-size: 34rpx;
  font-weight: 600;
  height: 88rpx;
  justify-content: center;
}
.fixed-botm-v2 .btn-cancel {
  background: #edf6ff;
  color: #0080ff;
  margin-right: 16rpx;
}
.fixed-botm-v2 .btn-confirm {
  background: #198cff;
  color: #fff;
  overflow: visible;
  position: relative;
}
.fixed-botm-v2 .btn-confirm .txt {
  line-height: 34rpx;
}
.fixed-botm-v2 .btn-confirm .timeout {
  color: #fff;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  line-height: 22rpx;
  margin-top: 6rpx;
  opacity: 0.8;
}
.train-smart-cs .common-popup .train-common-dialog-bd .desc {
  margin-left: 30rpx;
  margin-right: 30rpx;
}
