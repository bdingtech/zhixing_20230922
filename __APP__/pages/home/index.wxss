.index_tab-bd .index_tab_item.current ~ .scrollbar {
  z-index: 2;
}
.index_tab-bd .index_tab_item:nth-of-type(1).current ~ .scrollbar {
  transform: translateZ(0);
}
.index_tab-bd .index_tab_item:nth-of-type(2).current ~ .scrollbar {
  transform: translate3d(100%, 0, 0);
}
.index_tab-bd .index_tab_item:nth-of-type(3).current ~ .scrollbar {
  transform: translate3d(200%, 0, 0);
}
.index_tab-bd .index_tab_item:nth-of-type(4).current ~ .scrollbar {
  transform: translate3d(300%, 0, 0);
