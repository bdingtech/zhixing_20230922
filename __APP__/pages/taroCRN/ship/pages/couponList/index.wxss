@import "/pages/taroCRN/sub-vendors.wxss";
@import "/pages/taroCRN/sub-common/d21ca39141151ffc9630a87cc35d5e00.wxss";
.ship-coupon-item {
  background: #fff;
  border-radius: 24rpx;
  margin: 0 16rpx 16rpx;
  padding: 40rpx 32rpx 28rpx;
}
.sp-coupon-title {
  color: #333;
  font-family: PingFangSC-Semibold;
  font-size: 32rpx;
  font-weight: 600;
  height: 48rpx;
  letter-spacing: 0;
  line-height: 48rpx;
}
.sp-coupon-subtitle {
  color: #999;
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  height: 32rpx;
}
.sp-coupon-subtitle,
.sp-yuan {
  font-weight: 400;
  letter-spacing: 0;
}
.sp-yuan {
  font-size: 32rpx;
}
.sp-price,
.sp-yuan {
  color: #fe4839;
  font-family: ZX-Regular;
  height: 48rpx;
  line-height: 48rpx;
}
.sp-price {
  font-size: 48rpx;
  font-weight: 400;
  letter-spacing: 0;
}
.sp-line {
  background: #ededed;
  height: 2rpx;
  margin: 40rpx 0 26rpx;
  width: 100%;
}
.sp-remark {
  position: relative;
}
.sp-remark-txt {
  color: #999;
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  font-weight: 400;
  letter-spacing: 0;
  line-height: 32rpx;
  margin-right: 52rpx;
}
page {
  height: 100vh;
}
.ship-coupon-list {
  background: #f5f5f5;
  height: 100%;
  position: relative;
}
.ship-coupon-list-content {
  flex: 1;
  min-height: 2rpx;
  position: relative;
  width: 100%;
  z-index: 1;
}
.unselect-coupon {
  background: #fff;
  border-radius: 24rpx;
  height: 108rpx;
  margin: 16rpx;
}
.unselect-txt {
  color: #198cff;
  font-family: PingFangSC-Medium;
  font-size: 34rpx;
  font-weight: 500;
  height: 48rpx;
  letter-spacing: 0;
  line-height: 48rpx;
}
