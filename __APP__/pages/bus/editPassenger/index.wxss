  position: fixed;
  top: 0;
  width: 100%;
  z-index: 100;
}
.bus-card-ocr-modal .cardocr-modal .modal-mask {
  animation: fade 0.2s linear forwards;
  background-color: rgba(0, 0, 0, 0.5);
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: -1;
}
.bus-card-ocr-modal .cardocr-modal .modal-mask.hidden {
  animation: fade-hidden 0.2s linear forwards;
}
@-webkit-keyframes fade-hidden {
  0% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
@keyframes fade-hidden {
  0% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
@-webkit-keyframes fade {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@keyframes fade {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
