.roll-light {
  animation: rotateLight 0.2s linear 0.3s both;
  display: block;
  height: 970rpx;
  left: 50%;
  margin: -485rpx 0 0-485rpx;
  position: absolute;
  top: 50%;
  width: 970rpx;
  z-index: 1000;
}
.dialogFadein {
  animation: dialogFadein 0.2s linear 0.3s both;
  background: 0;
}
.dialogFadein .dialog-con .bond {
  font-size: 24rpx;
  margin: 24px auto 10px;
}
@-webkit-keyframes rotateLight {
  0% {
    opacity: 0;
    transform: rotate(0);
  }
  80% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: rotate(180deg);
  }
}
@keyframes rotateLight {
  0% {
    opacity: 0;
    transform: rotate(0);
  }
  80% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: rotate(180deg);
  }
}
@-webkit-keyframes dialogFadein {
  0% {
    opacity: 0;
    transform: scale(0.3);
  }
  100% {
    opacity: 1;
    transform: scale(1);
  }
}
@keyframes dialogFadein {
  0% {
    opacity: 0;
    transform: scale(0.3);
  }
  100% {
    opacity: 1;
    transform: scale(1);
  }
}
