.common-login-button {
  position: relative;
}
.common-login-button .virtual-btn {
  background: transparent;
  display: block;
  height: 100%;
  position: absolute;
  width: 100%;
  z-index: 10;
}
.rule-dialog-c {
  background: #fff;
  border-radius: 32rpx;
  padding: 40rpx 38rpx;
  width: 600rpx;
}
.rule-dialog-c .title {
  color: #333;
  font-size: 40rpx;
  font-weight: 600;
  line-height: 56rpx;
  text-align: center;
}
.rule-dialog-c .tips {
  color: #666;
  font-size: 28rpx;
  line-height: 42rpx;
  margin-top: 30rpx;
  text-align: center;
}
.rule-dialog-c .txt {
  color: #0080ff;
}
.rule-dialog-c .btn {
  border-radius: 16rpx;
  color: #fff;
  flex: 1;
  font-size: 32rpx;
  font-weight: 500;
  height: 88rpx;
  line-height: 88rpx;
  margin-right: 20rpx;
  margin-top: 40rpx;
}
.rule-dialog-c .btn:last-child {
  margin-right: 0;
}
.rule-dialog-c .cancle {
  background-color: #f5f5f5;
  color: #666;
  font-weight: 400;
}
.rule-dialog-c .btn-wechat,
.rule-dialog-c .common-login-button {
  flex: 1;
}
.rule-dialog-c .btn-wechat {
  background: #0080ff;
  border-radius: 16rpx;
  color: #fff;
  font-size: 32rpx;
  font-weight: 500;
  height: 88rpx;
  line-height: 88rpx;
  margin-right: 20rpx;
  margin-top: 40rpx;
}
.account-login {
  background: #fff;
  height: 100vh;
}
.login-top {
  font-size: 52rpx;
  font-weight: 600;
  line-height: 72rpx;
  padding: 200rpx 0 60rpx;
  text-align: center;
  white-space: pre-wrap;
}
.login-top .header-icon {
  display: block;
  height: 140rpx;
  margin: 0 auto 36rpx;
  width: 140rpx;
}
.login-top .welcome-text {
  background-position: 0 100%;
  background-repeat: no-repeat;
  background-size: 200rpx 20rpx;
}
.zx .login-top .welcome-text {
  background-image: url(https://images3.c-ctrip.com/ztrip/train_bin/22-05/img_hengxian@3x.png);
}
.ty .login-top .welcome-text {
  background-image: url(https://images3.c-ctrip.com/ztrip/train_bin/22-05/img_hengxian_tieyou@3x.png);
}
.login-wechat {
  margin: 0 60rpx;
}
.login-wechat .btn-line-grey,
.login-wechat .btn-wechat {
  background: #f2f9ff;
  border: none;
  border-radius: 28rpx;
  color: #0080ff;
  height: 100rpx;
  line-height: 100rpx;
  margin-bottom: 40rpx;
}
.login-wechat .btn-wechat {
  background: #0080ff;
  color: #fff;
  font-size: 36rpx;
  font-weight: 600;
}
.login-wechat .btn-wechat.swan {
  background: #198cff;
}
.login-wechat .ifont-wechat {
  font-size: 54rpx;
  font-weight: 400;
  margin: -6rpx 12rpx 0 0;
}
.login-input {
  margin: 0 60rpx;
}
.login-input .item {
  -ms-flex-align: center;
  align-items: center;
  border-bottom: 2rpx solid #eee;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.login-input .input-txt {
  flex: 1;
  font-size: 36rpx;
  font-weight: 600;
  height: 110rpx;
}
.login-input .input-plder {
  color: #ccc;
  font-weight: 400;
}
.login-input .btn-get {
  background: #fff;
  border-radius: 0;
  font-size: 32rpx;
  font-weight: 600;
  height: 88rpx;
  line-height: 88rpx;
}
.ztrip-h5 .btn-get {
  width: auto;
}
.login-input .txt {
  font-size: 32rpx;
  font-weight: 600;
}
.login-input .txt.ty {
  color: #fc6e51;
}
.login-input .txt.zx {
  color: #0080ff;
}
.login-input .btn-login {
  border-radius: 100rpx;
  font-size: 36rpx;
  font-weight: 600;
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 60rpx;
}
.btn-login.zx {
  background: #0080ff;
}
.btn-login.ty {
  background: #fc6e51;
}
.login-tips {
  bottom: 72rpx;
  bottom: calc(72rpx + env(safe-area-inset-bottom));
  color: #999;
  font-size: 24rpx;
  left: 60rpx;
  position: absolute;
  right: 60rpx;
  text-align: center;
}
.login-tips .txt {
  color: #0080ff;
}
.login-tips .radio {
  background-repeat: no-repeat;
  background-size: contain;
  display: inline-block;
  height: 30rpx;
  margin-right: 14rpx;
  position: relative;
  top: 6rpx;
  width: 30rpx;
}
.zx .login-tips .radio.selected {
  background-image: url(https://images3.c-ctrip.com/ztrip/train_bin/22-05/icon_yixuanze@3x.png);
}
.ty .login-tips .radio.selected {
  background-image: url(https://images3.c-ctrip.com/ztrip/train_bin/22-05/icon_tieyou_yixuan@3x.png);
}
.login-tips .radio.unselected {
  background-image: url(https://images3.c-ctrip.com/ztrip/train_bin/22-05/icon_weixuanze@3x.png);
}
.toast {
  background: rgba(0, 0, 0, 0.85);
  border-radius: 12rpx;
  color: #fff;
  font-size: 28rpx;
  left: 50%;
  padding: 20rpx 30rpx;
  position: fixed;
  top: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
  z-index: 100;
}
.login-top .swan-title-wrapper {
  text-align: center;
}
.login-top .swan-title-wrapper .logo {
  height: 100rpx;
  width: 100rpx;
}
.login-top .swan-title-wrapper .title {
  color: #212121;
  font-family: PingFangSC-Semibold;
  font-size: 36rpx;
  font-weight: 600;
}
