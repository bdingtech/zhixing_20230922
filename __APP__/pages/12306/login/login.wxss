.t6-sync-switch {
  background: #f5faff;
  border-radius: 24rpx;
  padding: 20rpx 24rpx;
}
.t6-sync-switch .title {
  color: #3c5a86;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  font-weight: 400;
  height: 40rpx;
  letter-spacing: 0;
  line-height: 40rpx;
}
.t6-sync-switch .switch-ui {
  background: #f5f5f5;
  border-radius: 14rpx;
  height: 40rpx;
  position: relative;
  width: 68rpx;
}
.t6-sync-switch .switch-ui::before {
  background: #fff;
  border-radius: 10rpx;
  content: "";
  height: 28rpx;
  left: 6rpx;
  margin-top: -14rpx;
  position: absolute;
  top: 50%;
  width: 28rpx;
}
.t6-sync-switch .switch-ui.checked {
  background: #198cff;
}
.t6-sync-switch .switch-ui.checked::before {
  left: calc(100% - 34rpx);
}
