page {
  background-color: #fff !important;
}
.routeplan-search-sd-location {
  font-size: 15px;
  background: #efefef;
  position: relative;
  min-height: 100vh;
  background: #fff;
  box-sizing: border-box;
}
.routeplan-search-content {
  height: 42px;
  background: #efefef;
  margin: 0 14px 10px;
  border-radius: 7px;
}
.routeplan-search-content-icon {
  width: 38px;
  height: 38px;
  margin: 2px 0;
}
.routeplan-search-content-ipt {
  height: 42px;
  width: 70%;
  padding-left: 15px;
}
.routeplan-search-clear-ipt {
  width: 20px;
  height: 20px;
  margin-top: 10px;
}
.routeplan-search-clear {
  width: 33px;
  height: 43px;
  text-align: center;
}
.routeplan-search-content-btn {
  height: 42px;
  line-height: 42px;
  padding: 0 10px;
}
.routeplan-search-empty {
  text-align: center;
}
.routeplan-search-choice {
  height: 52px;
}
.routeplan-search-flex {
  display: -webkit-flex;
  display: flex;
  justify-content: space-around;
}
.routeplan-search-choice-iocn {
  width: 20px;
  height: 20px;
  margin: 15px 5px;
  display: block;
}
.routeplan-search-choice-text {
  height: 52px;
  line-height: 52px;
}
.routeplan-search-linebox {
  width: 100%;
  height: 6px;
  background: #efefef;
}
.routeplan-search-res-list-item {
  padding: 10px 20px;
  border-bottom: 1px solid #efefef;
}
.routeplan-search-name {
  margin-bottom: 5px;
}
.routeplan-search-address {
  width: 90%;
  font-size: 13px;
  color: #666;
}
.routeplan-search-bottom {
  height: 34px;
  line-height: 34px;
  text-align: center;
}
.routeplan-history-content-item {
  width: 100%;
  height: 52px;
  line-height: 52px;
  padding: 0 0 15px 10px;
  border-bottom: 1px solid #efefef;
}
.routeplan-history-content-item-detail {
  display: -webkit-flex;
  display: flex;
}
.routeplan-history-content-item-header {
  display: inline-block;
  width: 85%;
}
.routeplan-history-content-item-text {
  height: 20px;
  width: 100%;
}
.routeplan-history-content-item-iocn {
  width: 20px;
  height: 20px;
  margin-top: 15px;
  margin-right: 4px;
  display: block;
}
.routeplan-history-content-item-address {
  font-size: 13px;
  color: #888;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.routeplan-clear-history {
  height: 80px;
  padding-top: 10px;
}
.routeplan-clear-history-btn {
  height: 30px;
  width: 134px;
  font-size: 13px;
  color: #666;
  border-radius: 16px;
  border: 1px solid #c7c7c7;
  margin: 0 auto;
  text-align: center;
  line-height: 30px;
}
.routeplan-empty-history {
  width: 100%;
  padding: 100px 0;
  text-align: center;
}
.routeplan-empty-history-title1 {
  margin: 5px auto;
}
.routeplan-empty-history-title2 {
  margin: 0 auto;
  font-size: 13px;
  color: #666;
}
.routeplan-logo-content {
  font-size: 12px;
  color: #a8a8a8;
  letter-spacing: 0;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, -50%);
  height: 22px;
}
.routeplan-tencent-map-logo {
  width: 18px;
  height: 18px;
  vertical-align: top;
  margin-right: 6px;
}
