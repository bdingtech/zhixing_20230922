.tip-protocol-content {
  height: 100vh;
  width: 100vw;
}
.tip-protocol-box {
  background-color: #fff;
  border-radius: 16rpx 16rpx 0 0;
  margin: 0 auto;
  overflow-y: scroll;
  padding: 0 32rpx;
}
.tip-protocol-list-text {
  margin-top: 40rpx;
}
.tip-protocol-title {
  color: #4a4a4a;
  font-size: 28rpx;
  line-height: 44rpx;
  padding: 0 24rpx;
  text-align: left;
}
.tip-protocol-text {
  color: #9b9b9b;
  font-size: 24rpx;
  line-height: 34rpx;
  line-height: 1.5;
  margin-bottom: 5rpx;
  margin-top: 16rpx;
  padding: 0 24rpx;
  text-align: justify;
}
.tip-protocol-text-bold {
  font-weight: 700;
}
