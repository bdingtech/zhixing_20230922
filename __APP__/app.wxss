.zt-pop-container {
  -ms-flex-pack: center;
  -ms-flex-align: center;
  align-items: center;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  justify-content: center;
  z-index: 9999;
}
.zt-pop-container,
.zt-pop-container .zt-layer {
  bottom: 0;
  left: 0;
  position: fixed;
  right: 0;
  top: 0;
}
.zt-pop-container .zt-fade-in {
  background: rgba(0, 0, 0, 0.6);
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
  z-index: 9999;
}
.zt-pop-container .zt-pop-content {
  display: none;
  position: fixed;
  z-index: 10000;
}
.zt-pop-container .zt-normal {
  display: block;
}
.zt-pop-container .zt-scale {
  animation: scale-up-center 0.4s;
  display: block;
}
@-webkit-keyframes scale-up-center {
  0% {
    transform: scale(0.5);
  }
  100% {
    transform: scale(1);
  }
}
@keyframes scale-up-center {
  0% {
    transform: scale(0.5);
  }
  100% {
    transform: scale(1);
  }
}
.zt-pop-container .zt-comeup {
  animation: come-up 0.3s;
  display: block;
}
@-webkit-keyframes come-up {
  0% {
    opacity: 0;
    transform: translateY(100%);
  }
  40% {
    opacity: 0;
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes come-up {
  0% {
    opacity: 0;
    transform: translateY(100%);
  }
  40% {
    opacity: 0;
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
.zt-pop-container .zt-falling {
  animation: fall 0.3s;
  display: block;
}
@-webkit-keyframes fall {
  0% {
    opacity: 0;
    transform: translateY(-100%);
  }
  40% {
    opacity: 0;
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes fall {
  0% {
    opacity: 0;
    transform: translateY(-100%);
  }
  40% {
    opacity: 0;
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
.zt-pop-container .zt-icon-closed {
  bottom: -180rpx;
  left: 50%;
  position: absolute;
  transform: translateX(-50%);
  z-index: 2;
}
.zt-drawer-container .zt-layer {
  background: #000;
  opacity: 0;
  position: fixed;
  z-index: 9999;
}
.zt-drawer-container .zt-layer.zt-fade-in {
  bottom: 0;
  left: 0;
  opacity: 0.6;
  right: 0;
  top: 0;
}
.zt-drawer-container .zt-drawer {
  background: #fff;
  border-radius: 16rpx 16rpx 0 0;
  bottom: 0;
  left: 0;
  max-height: 80%;
  min-height: 100rpx;
  position: fixed;
  right: 0;
  transform: translateY(200%);
  transition: -webkit-transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out;
  transition: transform 0.3s ease-in-out, -webkit-transform 0.3s ease-in-out;
  z-index: 10000;
}
.zt-drawer-container .zt-drawer.zt-drawer-show {
  transform: translateY(0);
}
.zt-drawer-container .zt-drawer .zt-icon-closed {
  height: 40rpx;
  position: absolute;
  right: 40rpx;
  top: 25rpx;
  width: 40rpx;
  z-index: 10000;
}
.zt-drawer-container .zt-drawer-header {
  border-radius: 16rpx 16rpx 0 0;
  line-height: 1.5;
  padding: 25rpx 30rpx;
  position: relative;
}
